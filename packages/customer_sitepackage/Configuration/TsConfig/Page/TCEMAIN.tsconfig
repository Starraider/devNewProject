TCEMAIN {
    table{
        tt_content {
            # Pages will *not* have "(copy)" appended:
            disablePrependAtCopy = 1
            # Pages will *not* be hidden upon copy:
            disableHideAtCopy = 1
        }
        pages {
            # Pages will *not* have "(copy)" appended:
            disablePrependAtCopy = 1
            # Pages will *not* be hidden upon copy:
            disableHideAtCopy = 1
        }
    }
    permissions {
        userid = 1
        groupid = 1
    }
}
