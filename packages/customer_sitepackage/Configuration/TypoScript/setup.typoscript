<INCLUDE_TYPOSCRIPT: source="FILE:EXT:bootstrap_package/Configuration/TypoScript/setup.typoscript">
<INCLUDE_TYPOSCRIPT: source="FILE:EXT:customer_sitepackage/Configuration/TypoScript/Extension/Form/setup.typoscript" condition="extension.isLoaded('form') == 1">

plugin.tx_typo3encore {
  settings {
    entrypointJsonPath = EXT:customer_sitepackage/Resources/Public/entrypoints.json
    manifestJsonPath = EXT:customer_sitepackage/Resources/Public/manifest.json
  }
}

##############
#### PAGE ####
##############
page {
  includeCSS {
    theme >
  }
  includeCSSLibs {
    googlewebfont >
    bootstrappackageicon >
  }

  includeJSLibs {
    head = typo3_encore:head
    head.forceOnTop = 1
    modernizr >
  }

  includeJS {

  }

  includeJSFooterlibs {
    jquery >
    #photoswipe_core >
    #photoswipe_ui >

    popper >
    bootstrap >
    bootstrap_accordion >
    bootstrap_popover >
    bootstrap_stickyheader >
    bootstrap_smoothscroll >
    bootstrap_lightbox >
    bootstrap_navbar >

  }

  includeJSFooter {
    customer_sitepackage_scripts >
  }
}
